# Rename this file to .env and adjust the paths and settings as needed

# Local directory where the FTP scanner writes files
WATCH_DIR=/path/to/ftp_dir

# Local directory where processed files will be moved
DEST_DIR=/path/to/dest_dir

# Ownership and permissions for processed files
OWNER=newuser:newgroup
PERMS=0644

# Seconds to wait for file size stabilization
STABLE_INTERVAL=30
